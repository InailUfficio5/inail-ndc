l0:identifier a owl:DatatypeProperty .

l0:name a owl:DatatypeProperty .

### Classes

CLV:Address a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty CLV:hasAddressComponent ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty CLV:hasNumber ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty CLV:hasStreetToponym ;
            owl:someValuesFrom owl:Thing ] ;
    owl:disjointWith CLV:AddressComponent .

CLV:AddressComponent a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf CLV:hasAddressComponent ] ;
            owl:someValuesFrom owl:Thing ] .

CLV:AdminUnitComponent a owl:Class ;
    rdfs:subClassOf CLV:AddressComponent,
        [ a owl:Restriction ;
            owl:onProperty l0:identifier ;
            owl:someValuesFrom rdfs:Literal ],
        [ a owl:Restriction ;
            owl:onProperty l0:name ;
            owl:someValuesFrom rdfs:Literal ] ;
    owl:disjointWith CLV:CivicNumbering .

CLV:City a owl:Class ;
    rdfs:subClassOf CLV:AdminUnitComponent .

CLV:CivicNumbering a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty CLV:streetNumber ;
            owl:someValuesFrom rdfs:Literal ],
        [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf CLV:hasNumber ] ;
            owl:someValuesFrom owl:Thing ] .

CLV:Country a owl:Class ;
    rdfs:subClassOf CLV:AdminUnitComponent .

CLV:Feature a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INAILCore:haLuogoDiNascita ] ;
            owl:someValuesFrom owl:Thing ] .

CLV:Province a owl:Class ;
    rdfs:subClassOf CLV:AdminUnitComponent .

CLV:StreetToponym a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty CLV:officialStreetName ;
            owl:someValuesFrom rdfs:Literal ],
        [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf CLV:hasStreetToponym ] ;
            owl:someValuesFrom owl:Thing ] ;
    rdfs:subClassOf CLV:AddressComponent .

COV:Organization a owl:Class .

CPV:Person a owl:Class .

INAILCore:Armatore a owl:Class ;
    owl:equivalentClass [ a owl:Class ;
            owl:unionOf (
                    INAILCore:ArmatoreResidenteAllEstero
                    INAILCore:ArmatoreResidenteInItalia ) ],
        [ a owl:Restriction ;
            owl:onProperty INAILCore:armatoreNominaCapitano ;
            owl:someValuesFrom owl:Thing ] ;
    rdfs:label "Armatore"@it,
        "Person who Fitts a Ship Out"@en ;
    rdfs:subClassOf INAILCore:SoggettoAssicuranteINAIL ;
    rdfs:comment """L’armatore è colui che assume l’esercizio della nave divenendo di conseguenza titolare di tutti i rapporti giuridici che sono connessi a tale esercizio. Ai fini del regolamento per il collocamento della gente di mare (Art. 2, comma 1, lettera b), DPR 231/2006) per armatore si intende la persona fisica o il soggetto giuridico che esercita l'impresa di navigazione ai sensi dell'articolo 265 del CdN.\r
Elemento essenziale che caratterizza lo status di armatore è la disponibilità della nave sulla base di un rapporto di natura reale (uso, usufrutto, proprietà ecc.) o di natura obbligatoria (comodato, locazione, ecc.). L'armatore può essere una persona fisica, una persona giuridica, un’associazione o una società di armamento."""@it .

INAILCore:ArmatoreResidenteAllEstero a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty INAILCore:armatoreHaIntermediario ;
            owl:someValuesFrom owl:Thing ] ;
    rdfs:label """Armatore\r
Residente\r
Estero"""@it,
        "Person who Prepare a Ship to Sail and Resident Abroad"@en ;
    owl:disjointWith INAILCore:ArmatoreResidenteInItalia ;
    rdfs:comment "L’armatore straniero – persona fisica residente all’estero o soggetto diverso da persona fisica che abbia sede all’estero – può armare navi battenti bandiera italiana, per il cui equipaggio vige l’obbligo assicurativo ed ha, altresì, facoltà di richiedere all’Inail l’assicurazione dell’equipaggio di una nave battente bandiera estera, ossia iscritta presso compartimenti esteri, purché siano soddisfatti i requisiti di iscrizione ai Registri e di composizione dell’equipaggio previsti, cioè le navi siano iscritte presso gli appositi Registri Speciali tenuti presso le Direzioni Marittime, ovvero presso la Sezione terza del Registro Internazionale e purché l’equipaggio sia costituito per almeno due terzi, da marittimi di cittadinanza italiana. "@it .

INAILCore:ArmatoreResidenteInItalia a owl:Class ;
    rdfs:label """Armatore\r
Residente\r
In Italia"""@it,
        "Person who Prepare a Ship to Sail and Italian Resident"@en ;
    rdfs:comment "Un armatore italiano può armare sia navi battenti bandiera italiana (per il cui equipaggio vige l’obbligo assicurativo) che navi battenti bandiera estera (art. 291 DPR 1124/1965) purché queste ultime siano iscritte presso gli appositi Registri Speciali tenuti presso le Direzioni Marittime, ovvero presso la Sezione terza del Registro Internazionale e purché l’equipaggio sia costituito per almeno due terzi, da marittimi di cittadinanza italiana."@it .

INAILCore:Associazione a owl:Class ;
    rdfs:label "Association"@en,
        "Associazione"@it ;
    rdfs:comment "L'associazione è un ente caratterizzato dall’organizzazione di più persone al fine di perseguire uno scopo comune"@it ;
    rdfs:isDefinedBy "https://w3id.org/italia/controlled-vocabulary/classifications-for-organizations/legal-status/1530"^^xsd:anyURI .

