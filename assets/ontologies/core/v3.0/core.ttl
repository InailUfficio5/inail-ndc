INAILCore:StatoDiSoggetto a owl:Class ;
    rdfs:label "Stato Di Soggetto"@it,
        "Subject Status"@en ;
    rdfs:subClassOf INAILCore:StatoPassato,
        [ a owl:Class ;
            owl:unionOf (
                    INAILCore:StatoDiPersonaFisica
