    rdfs:subClassOf INAILCore:PersonaGiuridica ;
    rdfs:comment "L'associazione è un ente caratterizzato dall’organizzazione di più persone al fine di perseguire uno scopo comune"@it ;
    rdfs:isDefinedBy "https://w3id.org/italia/controlled-vocabulary/classifications-for-organizations/legal-status/1530"^^xsd:anyURI .

INAILCore:AssociazioneNonRiconosciuta a owl:Class ;
    rdfs:label "Associazione Non Riconosciuta"@it,
        "Unrecognized Association"@en ;
    rdfs:subClassOf INAILCore:Associazione ;
