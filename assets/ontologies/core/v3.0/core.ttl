            owl:onProperty INAILCore:dataCostituzione ;
            owl:someValuesFrom rdfs:Literal ],
        [ a owl:Restriction ;
            owl:onProperty INAILCore:dataInizioAttivita ;
            owl:someValuesFrom rdfs:Literal ],
        [ a owl:Restriction ;
            owl:onProperty INAILCore:haClassificazioneATECO ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty INAILCore:presentaDenuncia ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty INAILCore:soggettoAssicuranteAssicura ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty INAILCore:soggettoAssicuranteCompetenzaDi ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty INAILCore:soggettoAssicuranteHaDimensione ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty INAILCore:soggettoAssicuranteHaFasciaNumeroLavoratori ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty INAILCore:soggettoAssicuranteHaStatoRapportoAssicurativo ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty INAILCore:statoAttivitaSoggettoAssicurante ;
            owl:someValuesFrom rdfs:Literal ],
        [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INAILCore:denunciaRelativaASoggettoAssicurante ] ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INAILCore:panAppartieneASoggettoAssicurante ] ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INAILCore:retribuzioneRelativaASoggettoAssicurante ] ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INAILCore:ricopreRuoloDiAssicurato ] ;
            owl:someValuesFrom owl:Thing ] ;
    owl:disjointWith INAILCore:StatoDiSoggettoAssicurante ;
    rdfs:comment "Soggetto di diritto (persona fisica o persona giuridica) titolare di un'attività lavorativa per la quale è obbligatoria l'assicurazione presso l'INAIL e in capo al quale la legge pone l'obbligo di effettuare le denunce obbligatorie e di pagare il premio assicurativo. [artt. 9, 10, 12, 28 TU 1124/1965]"@it .

INAILCore:SoggettoAssicuranteSospesoINAIL a owl:Class ;
    rdfs:label "INAIL Suspended Insuring Subject"@en,
        "Soggetto Assicurante  Sospeso INAIL"@it ;
    rdfs:subClassOf INAILCore:SoggettoAssicuranteINAIL ;
    rdfs:comment "Soggetto assicurante sospeso"@it .

INAILCore:SoggettoAssicuratoINAIL a owl:Class ;
    rdfs:label "INAIL Insured Subject"@en,
        "Soggetto Assicurato INAIL"@it ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty INAILCore:categoriaSoggettoAssicurato ;
            owl:someValuesFrom rdfs:Literal ],
        [ a owl:Restriction ;
            owl:onProperty INAILCore:dataInizioRischio ;
            owl:someValuesFrom rdfs:Literal ],
        [ a owl:Restriction ;
            owl:onProperty INAILCore:numeroGiornateSoggettoAssicurato ;
            owl:someValuesFrom rdfs:Literal ],
        [ a owl:Restriction ;
            owl:onProperty INAILCore:soggettoAssicuranteVariataIn ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty INAILCore:soggettoAssicuratoHaRetribuzione ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty INAILCore:soggettoAssicuratoSvolgeAttivita ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty INAILCore:tipoAssicurato ;
            owl:someValuesFrom rdfs:Literal ],
        [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INAILCore:ricopreRuolodiAssicurante ] ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INAILCore:ricopreRuoloDiAssicurato ] ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INAILCore:soggettoAssicuranteAssicura ] ;
            owl:someValuesFrom owl:Thing ] ;
    rdfs:comment "Sono assicurati all'Inail, in linea generale, tutti i soggetti che svolgono lavoro retribuito alle dipendenze di un datore di lavoro e siano addetti ad una delle attività indicate dall'articolo 1 (per il settore industria) e dagli articoli 204 e seguenti (per il settore agricoltura) del Testo Unico (d.P.R. 1124/1965). Sono assicurate anche alcune categorie di lavoratori autonomi (quali, ad esempio, gli artigiani e i coltivatori diretti), i lavoratori parasubordinati (articolo 4 d.lgs. 38/2000) e altri lavoratori assicurati ai sensi di disposizioni speciali (soggetti di cui alla legge 493/1999, medici e tecnici di radiologia di cui alla legge 93/1958). "@it .

INAILCore:StatoDiLuogo a owl:Class ;
    rdfs:label "Location Status"@en,
        "Stato Di Luogo"@it ;
    rdfs:subClassOf INAILCore:StatoPassato ;
    owl:disjointWith INAILCore:StatoDiSoggetto ;
    rdfs:comment "Stato di luogo"@it .

INAILCore:StatoDiPersonaFisica a owl:Class ;
    rdfs:label "Individual Status"@en,
        "Stato Di Persona Fisica"@it ;
    rdfs:subClassOf INAILCore:StatoDiSoggetto,
        [ a owl:Restriction ;
            owl:onProperty INAILCore:cognomePersona ;
            owl:someValuesFrom rdfs:Literal ],
        [ a owl:Restriction ;
            owl:onProperty INAILCore:nomePersona ;
            owl:someValuesFrom rdfs:Literal ],
        [ a owl:Restriction ;
            owl:onProperty INAILCore:sesso ;
            owl:someValuesFrom rdfs:Literal ] ;
    rdfs:comment "Stato di persona fisica"@it .

INAILCore:StatoDiPersonaGiuridica a owl:Class ;
