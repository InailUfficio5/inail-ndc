            owl:someValuesFrom rdfs:Literal ],
        [ a owl:Restriction ;
            owl:onProperty INAILCore:dataInizioAttivitaSoggettoAssicurato ;
            owl:someValuesFrom rdfs:Literal ],
        [ a owl:Restriction ;
            owl:onProperty INAILCore:descrizioneAttivita ;
            owl:someValuesFrom rdfs:Literal ],
        [ a owl:Restriction ;
            owl:onProperty INAILCore:lavorazioniComplementariESussidiarie ;
            owl:someValuesFrom rdfs:Literal ],
        [ a owl:Restriction ;
            owl:onProperty INAILCore:macchinario ;
            owl:someValuesFrom rdfs:Literal ],
        [ a owl:Restriction ;
            owl:onProperty INAILCore:mezzoDiTrasporto ;
            owl:someValuesFrom rdfs:Literal ],
        [ a owl:Restriction ;
            owl:onProperty INAILCore:percentualeIncidenza ;
            owl:someValuesFrom rdfs:Literal ],
        [ a owl:Restriction ;
            owl:onProperty INAILCore:prodottoRealizzato ;
