INAILCore:SoggettoAssicuranteCessatoINAIL a owl:Class ;
    rdfs:label "INAIL Ceased Insuring Subject"@en,
        "Soggetto Assicurante Cessato INAIL"@it ;
    rdfs:subClassOf INAILCore:SoggettoAssicuranteINAIL ;
    rdfs:comment "Soggetto assicurante cessato"@it .

INAILCore:SoggettoAssicuranteINAIL a owl:Class ;
    rdfs:label "INAIL Insuring Subject"@en,
        "Soggetto Assicurante INAIL"@it ;
    rdfs:subClassOf [ a owl:Class ;
            owl:unionOf (
                    INAILCore:SoggettoAssicuranteAttivoINAIL
