        [ a owl:Restriction ;
            owl:onProperty INAILCore:ragioneSociale ;
            owl:someValuesFrom rdfs:Literal ] ;
    owl:disjointWith INAILCore:StatoDiPersonaGiuridica .

INAILCore:PiccolaSocietaCooperativa a owl:Class ;
