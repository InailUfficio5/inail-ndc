### Object Properties

INAIL_Seamen:certificatoRelativoAEquipaggio a owl:ObjectProperty ;
    rdfs:label "certificato relativo a equipaggio"@it ;
    rdfs:domain INAIL_Seamen:CertificatoAssicurazioneEquipaggio ;
    rdfs:range INAIL_Seamen:EquipaggioNave .

INAIL_Seamen:certificatoRichiestoTramite a owl:ObjectProperty ;
    rdfs:label "certificato richiesto tramite"@it ;
    rdfs:domain INAIL_Seamen:CertificatoAssicurazioneEquipaggio ;
    rdfs:range INAILCore:DenunciaDiIscrizionePAN .

INAIL_Seamen:costituitaDa a owl:ObjectProperty ;
    rdfs:label "costituita da"@it ;
    rdfs:domain INAIL_Seamen:TabellaMinimaDiArmamento ;
    rdfs:range INAIL_Seamen:MembroEquipaggioNave .

INAIL_Seamen:denunciaMarittimiHaVariazione a owl:ObjectProperty ;
    rdfs:label "denuncia marittimi ha variazione"@it ;
    rdfs:domain INAILCore:DenunciaDiVariazionePAN ;
    rdfs:range INAIL_Seamen:VariazionePAN .

INAIL_Seamen:equipaggioCostituitoDaMembro a owl:ObjectProperty ;
    rdfs:label "equipaggio costituito da membro"@it ;
    rdfs:domain INAIL_Seamen:EquipaggioNave ;
    rdfs:range INAIL_Seamen:MembroEquipaggioNave .

INAIL_Seamen:equipaggioRelativoANave a owl:ObjectProperty ;
    rdfs:label "equipaggio relativo a nave"@it ;
    rdfs:domain INAIL_Seamen:EquipaggioNave ;
    rdfs:range INAILAutoliq:Nave .

INAIL_Seamen:polizzaMarittimiRelativaA a owl:ObjectProperty ;
    rdfs:label "polizza marittimi relativa a"@it ;
    rdfs:domain INAILCore:PolizzaMarittimi ;
    rdfs:range INAIL_Seamen:EquipaggioNave .

INAIL_Seamen:stabilitaDaAutorita a owl:ObjectProperty ;
    rdfs:label "stabilita da autorità"@it ;
    rdfs:domain INAIL_Seamen:TabellaMinimaDiArmamento ;
    rdfs:range COV:Organization .

INAIL_Seamen:variazioneDatiCaratteristiciRelativaANave a owl:ObjectProperty ;
    rdfs:label "variazione dati caratteristici relativa a nave"@it ;
    rdfs:domain INAIL_Seamen:VariazioneDatiCaratteristiciNave ;
    rdfs:range INAILAutoliq:Nave .

INAIL_Seamen:variazioneDatiRelativaASoggettoAssicurante a owl:ObjectProperty ;
    rdfs:label "variazione dati relativa a soggetto assicurante"@it ;
    rdfs:domain INAIL_Seamen:VariazioneDatiSoggettoAssicuranteINAIL ;
    rdfs:range INAILCore:SoggettoAssicuranteINAIL .

INAIL_Seamen:variazioneDatiTecniciRelativaANave a owl:ObjectProperty ;
    rdfs:label "variazione dati tecnici relativa a nave"@it ;
    rdfs:domain INAIL_Seamen:VariazioneDatiTecniciNave ;
    rdfs:range INAILAutoliq:Nave .

INAIL_Seamen:variazioneRelativaAProprietario a owl:ObjectProperty ;
    rdfs:label "variazione relativa a proprietario"@it ;
    rdfs:domain INAIL_Seamen:VariazioneProprietarioNave ;
    rdfs:range INAILAutoliq:ProprietarioNave .

INAIL_Seamen:variazioneRetribuzioneRelativaA a owl:ObjectProperty ;
    rdfs:label "variazione retribuzione relativa a"@it ;
    rdfs:domain INAIL_Seamen:VariazioneRetribuzioneSoggettoAssicuratoMarittimiINAIL ;
    rdfs:range INAILCore:RetribuzioneComplessiva .

INAILCore:denunciaRelativaAPAN a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:domain INAILCore:DenunciaPAN ;
    rdfs:range INAILCore:PosizioneAssicurativaNavigazione .

INAILCore:retribuzioneRelativaASoggettoAssicurante a owl:ObjectProperty ;
    rdfs:domain INAILCore:RetribuzioneComplessiva ;
    rdfs:range INAILCore:SoggettoAssicuranteINAIL .

INAILCore:tipoCertificatoRelativoAPAN a owl:ObjectProperty ;
    rdfs:domain INAILCore:TipoCertificatoPAN ;
    rdfs:range INAILCore:PosizioneAssicurativaNavigazione .

### Annotation Properties

dct:accessURL a owl:AnnotationProperty .

dct:description a owl:AnnotationProperty .

dct:downloadURL a owl:AnnotationProperty .

dct:format a owl:AnnotationProperty .

dct:identifier a owl:AnnotationProperty .

dct:license a owl:AnnotationProperty .

dct:title a owl:AnnotationProperty .

foaf:name a owl:AnnotationProperty .

vcard:fn a owl:AnnotationProperty .

vcard:hasEmail a owl:AnnotationProperty .

### Data Properties

INAIL_Seamen:gradoMembroEquipaggio a owl:DatatypeProperty ;
    rdfs:label "grado membro equipaggio"@it ;
    rdfs:domain INAIL_Seamen:MembroEquipaggioNave .

INAIL_Seamen:qualificaSoggettoAssicurato a owl:DatatypeProperty ;
    rdfs:label "qualifica soggetto assicurato"@it ;
    rdfs:domain INAIL_Seamen:EquipaggioNave .

### Classes

COV:Organization a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INAIL_Seamen:stabilitaDaAutorita ] ;
            owl:someValuesFrom owl:Thing ] .

INAIL_Seamen:CertificatoAssicurazioneEquipaggio a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty INAIL_Seamen:certificatoRelativoAEquipaggio ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty INAIL_Seamen:certificatoRichiestoTramite ;
            owl:someValuesFrom owl:Thing ] ;
    rdfs:label "Certificato assicurazione equipaggio"@it,
        "Crew insurance certificate"@en ;
    rdfs:comment "Certificato di assicurazione dell'equipaggio"@it .

INAIL_Seamen:CreazioneNuovaPAN a owl:Class ;
    rdfs:label "Creazione nuova PAN"@it,
        "New PAN creation"@en ;
    rdfs:comment "Sistema di variazione della PAN che consente di crearne una nuova."@it .

INAIL_Seamen:EquipaggioNave a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty INAIL_Seamen:equipaggioCostituitoDaMembro ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty INAIL_Seamen:equipaggioRelativoANave ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty INAIL_Seamen:qualificaSoggettoAssicurato ;
            owl:someValuesFrom rdfs:Literal ],
        [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INAIL_Seamen:certificatoRelativoAEquipaggio ] ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INAIL_Seamen:polizzaMarittimiRelativaA ] ;
            owl:someValuesFrom owl:Thing ] ;
    rdfs:label "Equipaggio nave"@it,
        "Ship crew"@en ;
    rdfs:subClassOf INAILCore:SoggettoAssicuratoINAIL ;
    rdfs:comment "Complesso del personale imbarcato a bordo di una nave"@it .

INAIL_Seamen:IstanzaRuoloUnico a owl:Class ;
    rdfs:label "Istanza ruolo unico"@it,
        "Unique role instance"@en ;
    rdfs:comment "Istanza per l'autorizzazione al \"Ruolo Unico\""@it .

INAIL_Seamen:MembroEquipaggioNave a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty INAIL_Seamen:gradoMembroEquipaggio ;
            owl:someValuesFrom rdfs:Literal ],
        [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INAIL_Seamen:costituitaDa ] ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INAIL_Seamen:equipaggioCostituitoDaMembro ] ;
            owl:someValuesFrom owl:Thing ] ;
    rdfs:label "Crew member"@en,
        "Membro equipaggio"@it ;
    rdfs:subClassOf INAILCore:SoggettoAssicuratoINAIL ;
    rdfs:comment "persona impiegata in un qualsiasi compito a bordo della nave, ed include qualsiasi persona assunta  direttamente,  sia  da un armatore sia tramite un'agenzia di armamento,  il  cui  luogo  di lavoro abituale e' a bordo della nave, compreso  il  comandante, gli ufficiali, i membri dell'equipaggio, ed il personale addetto al catering, e ai servizi di sala e alberghieri"@it .

INAIL_Seamen:TabellaMinimaDiArmamento a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty INAIL_Seamen:costituitaDa ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty INAIL_Seamen:stabilitaDaAutorita ;
            owl:someValuesFrom owl:Thing ] ;
    rdfs:label "Minimum fitting out table"@en,
        "Tabella minima di armamento"@it ;
    rdfs:comment "La tabella minima di armamento contiene il numero e i nomi dei componenti dell'equipaggio e i gradi corrispondenti alle qualifiche di bordo. Stabilita da una commissione presieduta dal comandante del porto, in essa rientrano i rappresentanti del sindacato e della società di armamento e i marittimi."@it .

INAIL_Seamen:VariazioneDatiCaratteristiciNave a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty INAIL_Seamen:variazioneDatiCaratteristiciRelativaANave ;
            owl:someValuesFrom owl:Thing ] ;
    rdfs:label "Ship characteristic data variation"@en,
        "Variazione dati caratteristici nave"@it ;
    rdfs:comment "Variazione dei dati caratteristici relativi alla nave (es: nome della nave, attività esercitata, servizio cui è abilitata ecc.)"@it .

INAIL_Seamen:VariazioneDatiPAN a owl:Class ;
    owl:equivalentClass [ a owl:Class ;
            owl:unionOf (
                    INAIL_Seamen:TabellaMinimaDiArmamento
                    INAIL_Seamen:VariazioneDatiCaratteristiciNave
                    INAIL_Seamen:VariazioneDatiSoggettoAssicuranteINAIL
                    INAIL_Seamen:VariazioneDatiTecniciNave
                    INAIL_Seamen:VariazioneProprietarioNave
                    INAIL_Seamen:VariazioneRetribuzioneSoggettoAssicuratoMarittimiINAIL ) ] ;
    rdfs:label "PAN data variation"@en,
        "Variazione dati PAN"@it ;
    rdfs:comment "Variazione dei dati della PAN"@it .

INAIL_Seamen:VariazioneDatiSoggettoAssicuranteINAIL a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty INAIL_Seamen:variazioneDatiRelativaASoggettoAssicurante ;
            owl:someValuesFrom owl:Thing ] ;
    rdfs:label "INAIL insuring subject data variation"@en,
        "Variazione dati soggetto assicurante INAIL"@it ;
    rdfs:comment "Variazione dei dati relativi al soggetto assicurante (es: ragione sociale)"@it .

INAIL_Seamen:VariazioneDatiTecniciNave a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty INAIL_Seamen:variazioneDatiTecniciRelativaANave ;
            owl:someValuesFrom owl:Thing ] ;
    rdfs:label "Ship technical data variation"@en,
        "Variazione dati tecnici nave"@it ;
    rdfs:comment "Variazione dei dati tecnici relativi alla nave (es: anno di costruzione, tonnellaggio ecc.)"@it .

INAIL_Seamen:VariazionePAN a owl:Class ;
    owl:equivalentClass [ a owl:Class ;
            owl:unionOf (
                    INAIL_Seamen:CreazioneNuovaPAN
                    INAIL_Seamen:IstanzaRuoloUnico
                    INAIL_Seamen:VariazioneDatiPAN ) ],
        [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INAIL_Seamen:denunciaMarittimiHaVariazione ] ;
            owl:someValuesFrom owl:Thing ] ;
    rdfs:label "PAN variation"@en,
        "Variazione PAN"@it ;
    rdfs:subClassOf TI:TimeIndexedEvent ;
    rdfs:comment "Indica una possibile variazione della PAN: la creazione di una nuova  oppure la variazione di una già esistente ecc."@it .

INAIL_Seamen:VariazioneProprietarioNave a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty INAIL_Seamen:variazioneRelativaAProprietario ;
            owl:someValuesFrom owl:Thing ] ;
    rdfs:label "Ship owner variation"@en,
        "Variazione proprietario nave"@it ;
    rdfs:comment "Variazione dei dati relativi al proprietario della nave (es: dati anagrafici, quote di proprietà ecc.)"@it .

INAIL_Seamen:VariazioneRetribuzioneSoggettoAssicuratoMarittimiINAIL a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty INAIL_Seamen:variazioneRetribuzioneRelativaA ;
            owl:someValuesFrom owl:Thing ] ;
    rdfs:label "INAIL seamen insured subject's salary variation"@en,
        "Variazione retribuzione soggetto assicurato marittimi INAIL"@it ;
    rdfs:comment "Variazione dei dati retributivi relativi al personale assicurato per le navi già armate"@it .

INAILAutoliq:Nave a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INAIL_Seamen:equipaggioRelativoANave ] ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INAIL_Seamen:variazioneDatiCaratteristiciRelativaANave ] ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INAIL_Seamen:variazioneDatiTecniciRelativaANave ] ;
            owl:someValuesFrom owl:Thing ] .

INAILAutoliq:ProprietarioNave a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INAIL_Seamen:variazioneRelativaAProprietario ] ;
            owl:someValuesFrom owl:Thing ] .

INAILCore:DenunciaDiCessazionePAN a owl:Class .

INAILCore:DenunciaDiIscrizionePAN a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INAIL_Seamen:certificatoRichiestoTramite ] ;
            owl:someValuesFrom owl:Thing ] .

INAILCore:DenunciaDiVariazionePAN a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty INAIL_Seamen:denunciaMarittimiHaVariazione ;
            owl:someValuesFrom owl:Thing ] .

INAILCore:DenunciaPAN a owl:Class ;
    owl:equivalentClass [ a owl:Class ;
            owl:unionOf (
                    INAILCore:DenunciaDiCessazionePAN
                    INAILCore:DenunciaDiIscrizionePAN
                    INAILCore:DenunciaDiVariazionePAN ) ],
        [ a owl:Restriction ;
            owl:onProperty INAILCore:denunciaRelativaAPAN ;
            owl:someValuesFrom owl:Thing ] .

INAILCore:PolizzaMarittimi a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty INAIL_Seamen:polizzaMarittimiRelativaA ;
            owl:someValuesFrom owl:Thing ] .

INAILCore:PosizioneAssicurativaNavigazione a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INAILCore:denunciaRelativaAPAN ] ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INAILCore:tipoCertificatoRelativoAPAN ] ;
            owl:someValuesFrom owl:Thing ] .

INAILCore:RetribuzioneComplessiva a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty INAILCore:retribuzioneRelativaASoggettoAssicurante ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INAIL_Seamen:variazioneRetribuzioneRelativaA ] ;
            owl:someValuesFrom owl:Thing ] .

INAILCore:SoggettoAssicuranteINAIL a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INAIL_Seamen:variazioneDatiRelativaASoggettoAssicurante ] ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INAILCore:retribuzioneRelativaASoggettoAssicurante ] ;
            owl:someValuesFrom owl:Thing ] .

INAILCore:SoggettoAssicuratoINAIL a owl:Class .

INAILCore:TipoCertificatoPAN a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty INAILCore:tipoCertificatoRelativoAPAN ;
            owl:someValuesFrom owl:Thing ] .

TI:TimeIndexedEvent a owl:Class .

