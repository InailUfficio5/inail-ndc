    adms:officialURI "https://w3id.org/italia/work-accident/onto/proc17_seamen"^^xsd:anyURI ;
    adms:prefix "INAIL_Seamen" ;
    adms:semanticAssetInUse <https://w3id.org/italia/data/project/NDC> ;
