    rdfs:domain INAIL_XrayMachine:DenunciaDiVariazionePolizzaApparecchiRXINAIL ;
    rdfs:range INAIL_XrayMachine:VariazioneSoggettoEspostoINAIL .

INAIL_XrayMachine:polizzaPerSoggettoEsposto a owl:ObjectProperty ;
