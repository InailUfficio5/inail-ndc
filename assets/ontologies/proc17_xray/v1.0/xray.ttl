INAIL_RX_Devices:variazioneRiferitaASoggettoEsposto a owl:ObjectProperty ;
    rdfs:label "variazione riferita a soggetto esposto"@it ;
    rdfs:domain INAIL_RX_Devices:VariazioneSoggettoEspostoINAIL ;
    rdfs:range INAIL_RX_Devices:SoggettoEspostoAApparecchioINAIL .

INAILCore:ricopreRuolo a owl:InverseFunctionalProperty,
