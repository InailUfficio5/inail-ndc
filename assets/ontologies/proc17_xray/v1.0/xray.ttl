### Object Properties

INAIL_RX_Devices:apparecchioRelativoAPolizza a owl:ObjectProperty ;
    rdfs:label "apparecchio relativo a polizza"@it ;
    rdfs:domain INAIL_RX_Devices:ApparecchioRXINAIL ;
    rdfs:range INAILCore:PolizzaApparecchiRX .

INAIL_RX_Devices:denunciaApparecchiRXRelativaA a owl:ObjectProperty ;
    rdfs:label "denuncia apparecchi RX relativa a"@it .

INAIL_RX_Devices:denunciaHaVariazioneApparecchioRX a owl:ObjectProperty ;
    rdfs:label "denuncia ha variazione apparecchio RX"@it ;
    rdfs:domain INAIL_RX_Devices:DenunciaDiVariazionePolizzaApparecchiRXINAIL ;
    rdfs:range INAIL_RX_Devices:VariazioneApparecchioRXINAIL .

INAIL_RX_Devices:denunciaHaVariazioneSoggettoEsposto a owl:ObjectProperty ;
    rdfs:label "denuncia ha variazione soggetto esposto"@it ;
    rdfs:domain INAIL_RX_Devices:DenunciaDiVariazionePolizzaApparecchiRXINAIL ;
    rdfs:range INAIL_RX_Devices:VariazioneSoggettoEspostoINAIL .

INAIL_RX_Devices:polizzaPerSoggettoEsposto a owl:ObjectProperty ;
    rdfs:label "polizza per soggetto esposto"@it ;
    rdfs:range INAIL_RX_Devices:SoggettoEspostoAApparecchioINAIL .

INAIL_RX_Devices:soggettoEspostoAdApparecchio a owl:ObjectProperty ;
    rdfs:label "soggetto esposto ad apparecchio"@it ;
    rdfs:domain INAIL_RX_Devices:SoggettoEspostoAApparecchioINAIL ;
    rdfs:range INAIL_RX_Devices:ApparecchioRXINAIL .

INAIL_RX_Devices:variazioneRiferitaAdApparecchio a owl:ObjectProperty ;
    rdfs:label "variazione riferita ad apparecchio"@it ;
    rdfs:domain INAIL_RX_Devices:VariazioneApparecchioRXINAIL ;
