INAILAutoliq:DocumentoDiBordoNave a owl:Class ;
    owl:equivalentClass [ a owl:Class ;
            owl:unionOf (
                    INAILAutoliq:CartaDiBordoNave
                    INAILAutoliq:CertificatoTecnicoNave
                    INAILAutoliq:DocumentoDoganaleNave
                    INAILAutoliq:DocumentoSanitarioNave
                    INAILAutoliq:GiornaleNautico
                    INAILAutoliq:InventarioDiBordoNave
                    INAILAutoliq:LibroDiBordoNave
                    INAILAutoliq:RegistroInfortuniNave ) ],
        [ a owl:Restriction ;
            owl:onProperty INAILAutoliq:abilitanoAllaNavigazione ;
            owl:someValuesFrom owl:Thing ] ;
    rdfs:label """Documento\r
Di Bordo Nave"""@it,
        "On-Board Ship Document"@en ;
    rdfs:comment """Documento obbligatorio per la navigazione e da tenere a bordo:\r
- le “carte di bordo” per le navi maggiori (l'atto di nazionalità e il ruolo di equipaggio) e per le navi minori e i galleggianti (la licenza).\r
Inoltre, le navi maggiori devono avere a bordo:\r
a) il certificato di stazza; il certificato di classe o quello di navigabilità; i certificati di bordo libero e di galleggiabilità; i certificati di visita;\r
b) i documenti doganali e sanitari;\r
c) il giornale nautico;\r
d) gli altri libri e documenti prescritti da leggi e regolamenti.\r
[art. 169 Codice della Navigazione]"""@it .

