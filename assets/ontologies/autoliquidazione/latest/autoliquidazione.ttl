        "Esattore"@it ;
    rdfs:subClassOf COV:Organization ;
    rdfs:comment "Persona incaricata della riscossione di somme dovute per obblighi contratti con un ente pubblico o privato."@it .

INAILAutoliq:GalleggianteMobile a owl:Class ;
    rdfs:label """Galleggiante\r
Mobile"""@it,
        "Mobile Float "@en ;
