COV:Organization a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty COV:legalName ;
            owl:someValuesFrom rdfs:Literal ],
        [ a owl:Restriction ;
            owl:onProperty COV:orgAcronym ;
            owl:someValuesFrom rdfs:Literal ],
        [ a owl:Restriction ;
            owl:onProperty INAILAutoliq:ricopreRuoloContribuente ;
            owl:someValuesFrom owl:Thing ] .

INAILAutoliq:AnnullamentoF24 a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty INAILAutoliq:annullamentoRelativoAF24 ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty INAILAutoliq:causaleAnnullamento ;
            owl:someValuesFrom rdfs:Literal ],
        [ a owl:Restriction ;
            owl:onProperty INAILAutoliq:dataAnnullamentoF24 ;
            owl:someValuesFrom rdfs:Literal ],
        [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INAILAutoliq:intermediarioRichiedeAnnullamento ] ;
            owl:someValuesFrom owl:Thing ] ;
    rdfs:label "Annullamento F24"@it,
        "F24 Cancellation"@en ;
    rdfs:comment "In caso di errore di compilazione o di sopravvenuta impossibilità ad eseguire il pagamento, è possibile annullare un modello F24 trasmesso attraverso i servizi telematici dell'Agenzia delle Entrate seguendo una apposita procedura on line, ma entro e non oltre il penultimo giorno lavorativo precedente la data di versamento prevista (cioè prima che il pagamento sia definitivamente inoltrato alla banca da parte dell’Agenzia delle Entrate). "@it .

INAILAutoliq:AssegnoBancarioOPostale a owl:Class ;
    rdfs:label """Assegno Bancario\r
O Postale"""@it,
        "Bank or Postal Check"@en ;
    rdfs:comment """Strumento di pagamento sostitutivo del contante con il quale il titolare del conto corrente (traente) ordina alla propria banca (trattario) di versare una determinata somma di denaro a favore di un’altra persona\r
Consente di effettuare pagamenti, anche di importo rilevante, senza utilizzare il denaro contante. Gli assegni postali sono equiparati ad un assegno bancario e possono essere utilizzati per effettuare pagamenti, trasferire denaro a terzi o prelevare contante presso il proprio Ufficio Postale."""@it .

INAILAutoliq:AssegnoCircolare a owl:Class ;
    rdfs:label "Assegno Circolare"@it,
        """Bank Draft\r
"""@en ;
    rdfs:comment "Titolo di credito all'ordine emesso da una banca autorizzata per somme che siano presso di esso disponibili al momento dell'emissione, e pagabile a vista presso tutti i recapiti comunque indicati dall'emittente. [Art. 82, R. D. 21 dicembre 1933, n. 1736] "@it .

INAILAutoliq:AttivitaVerificaImportiVersati a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INAILAutoliq:svolgeAttivita ] ;
            owl:someValuesFrom owl:Thing ] ;
    rdfs:label "Activities to Verify the Amounts Paid"@en,
        """Attivita Verifica\r
Importi Versati"""@it ;
    rdfs:comment "Attività eseguita dall'Inail, a seguito del pagamento dell'F24, che consiste nel verificare che l'importo pagato sia uguale a quello che viene calcolato come dovuto  internamente dall'archivio GRA (Gestione Rapporti Assicurativi). "@it .

INAILAutoliq:AttoDiNazionalitaNave a owl:Class ;
    rdfs:label """Atto Di\r
Nazionalità Nave"""@it,
        "Ship Nationality Act"@en ;
    rdfs:subClassOf INAILAutoliq:CartaDiBordoNave ;
    owl:disjointWith INAILAutoliq:RuoloDiEquipaggioNave ;
    rdfs:comment "L'atto di nazionalità è rilasciato dal direttore marittimo nella cui zona la nave maggiore è immatricolata, e, nel caso di cui all'articolo 148, dal console che ne ha ricevuto la iscrizione. L'atto di nazionalità enuncia il nome, il tipo e le caratteristiche principali, la stazza lorda e netta della nave, il nome del proprietario, l'ufficio di immatricolazione (Art. 150 CdN)."@it .

INAILAutoliq:BaseDiCalcolo a owl:Class ;
    owl:equivalentClass [ a owl:Class ;
            owl:unionOf (
                    INAILAutoliq:BaseDiCalcoloContributiAssociativi
                    INAILAutoliq:BaseDiCalcoloDipendenti
                    INAILAutoliq:BaseDiCalcoloNavigazione
                    INAILAutoliq:BaseDiCalcoloSoggettiAutonomiArtigiani ) ],
        [ a owl:Restriction ;
            owl:onProperty INAILAutoliq:AddizionaleAmianto ;
            owl:someValuesFrom rdfs:Literal ],
        [ a owl:Restriction ;
            owl:onProperty INAILAutoliq:AnnoDiRiferimentoBaseDiCalcolo ;
            owl:someValuesFrom rdfs:Literal ],
        [ a owl:Restriction ;
            owl:onProperty INAILAutoliq:oscillazioneTasso ;
            owl:someValuesFrom rdfs:Literal ],
        [ a owl:Restriction ;
            owl:onProperty INAILAutoliq:tassoApplicabile ;
            owl:someValuesFrom rdfs:Literal ],
        [ a owl:Restriction ;
            owl:onProperty INAILAutoliq:tassoApplicato ;
            owl:someValuesFrom rdfs:Literal ],
        [ a owl:Restriction ;
            owl:onProperty INAILAutoliq:tassoMedioDiTariffa ;
            owl:someValuesFrom rdfs:Literal ],
        [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INAILAutoliq:baseDiCalcoloLegataASoggettoAssicurante ] ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INAILAutoliq:costituisconoBaseDiCalcolo ] ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INAILAutoliq:denunciaDetermina ] ;
            owl:someValuesFrom owl:Thing ] ;
    rdfs:label "Base Di Calcolo"@it,
        "Calculation Base"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty INAILAutoliq:determinaPremioAssicurativo ;
            owl:someValuesFrom owl:Thing ] ;
    rdfs:comment "Individua gli elementi Insieme degli elementi necessari per il calcolo della regolazione e della rata del premio di autoliquidazione e dei contributi associativi. La comunicazione delle basi di calcolo del premio, per ogni codice ditta, avviene tramite pubblicazione nella sezione “Fascicolo aziende” mentre per il settore marittimo sono rese disponibili agli armatori direttamente con il servizio online \"Visualizzazione elementi calcolo\". "@it .

