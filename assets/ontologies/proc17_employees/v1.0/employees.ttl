### Classes

INAIL_Employees:DenunciaIscrizioneDipendentiINAIL a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty INAIL_Employees:denunciaIscrizioneRelativaADipendente ;
            owl:someValuesFrom owl:Thing ] ;
    rdfs:label "Denuncia iscrizione dipendenti INAIL"@it,
        "INAIL Enrollment complaint employees"@en ;
    rdfs:comment "Denuncia di iscrizione di una nuova ditta con contestuale apertura di una polizza assicurativa per lavoratori dipendenti e assimilati."@it .

INAIL_Employees:DenunciaVariazioneDipendentiINAIL a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty INAIL_Employees:denunciaDipendentiHaVariazione ;
            owl:someValuesFrom owl:Thing ] ;
    rdfs:label "Denuncia variazione dipendenti INAIL"@it,
        "INAIL variation complaint employees"@en ;
    rdfs:comment "Denuncia che consente di modificare i dati relativi ad un lavoratore dipendente o assimilato oppure l'inserimento di uno nuovo."@it .

INAIL_Employees:DipendenteINAIL a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty INAIL_Employees:dipendenteSvolgeAttivita ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INAIL_Employees:denunciaIscrizioneRelativaADipendente ] ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INAIL_Employees:polizzaDipendentiRelativaA ] ;
            owl:someValuesFrom owl:Thing ] ;
    rdfs:label "Dipendente INAIL"@it,
        "INAIL employee"@en ;
    rdfs:subClassOf INAILCore:SoggettoAssicuratoINAIL ;
    rdfs:comment "Soggetto che svolge un'attività di lavorazione e che ricopre il ruolo di dipendente lavoratore o assimilato."@it .

INAIL_Employees:VariazioneAttivitaDipendenteINAIL a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty INAIL_Employees:variazioneLavorazioneDipendenteRelativaA ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INAIL_Employees:denunciaDipendentiHaVariazione ] ;
            owl:someValuesFrom owl:Thing ] ;
    rdfs:label "INAIL activity variation employee"@en,
        "Variazione attività dipendente INAIL"@it ;
    rdfs:comment "Variazione dei dati relativi all'attività di lavorazione di un lavoratore dipendente o assimilato."@it .

INAILCore:Attivita a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INAIL_Employees:dipendenteSvolgeAttivita ] ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INAIL_Employees:variazioneLavorazioneDipendenteRelativaA ] ;
            owl:someValuesFrom owl:Thing ] .

INAILCore:PersonaFisica a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty INAILCore:ricopreRuoloDiAssicurato ;
            owl:someValuesFrom owl:Thing ] .

INAILCore:PolizzaDipendenti a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty INAIL_Employees:polizzaDipendentiRelativaA ;
            owl:someValuesFrom owl:Thing ] .

INAILCore:SoggettoAssicuratoINAIL a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty [ owl:inverseOf INAILCore:ricopreRuoloDiAssicurato ] ;
            owl:someValuesFrom owl:Thing ] .

